{
  "name": "@openzeppelin/compact-tools-cli",
  "private": true,
  "description": "CLI for compiling and building Compact smart contracts",
  "version": "0.0.1",
  "keywords": [
    "compact",
    "cli",
    "compiler",
    "builder",
    "testing"
  ],
  "author": "",
  "license": "MIT",
  "type": "module",
  "exports": "./index.js",
  "engines": {
    "node": ">=20"
  },
  "bin": {
    "compact-builder": "dist/runBuilder.js",
    "compact-compiler": "dist/runCompiler.js"
  },
  "scripts": {
