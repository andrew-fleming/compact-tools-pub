    "types": "tsc -p tsconfig.json --noEmit",
    "test": "yarn vitest run",
    "clean": "git clean -fXd"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.1",
    "@types/node": "24.9.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "log-symbols": "^7.0.0",
    "ora": "^9.0.0"
  }
}
